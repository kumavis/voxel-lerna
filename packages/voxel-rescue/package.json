{
  "name": "voxel-rescue",
  "description": "Automatically teleport the player back to a known location if they enter a certain region of the world (e.g. fall off the map)",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "git@github.com:robbles/voxel-rescue.git"
  },
  "dependencies": {
    "inherits": "1.0.0",
    "underscore": "1.4.3"
  },
  "devDependencies": {
    "browservefy": "0.0.1",
    "painterly-textures": "git://github.com/maxogden/painterly-textures.git#relative",
    "voxel-engine": "workspace:^"
  },
  "scripts": {
    "start": "echo \"open localhost:8080/\" && ./node_modules/.bin/browservefy demo.js 8080 -- -d"
  }
}
